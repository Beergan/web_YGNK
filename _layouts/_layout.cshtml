                    const wardExists = wards.some(w => w.code === selectedWard);
                    if (wardExists) {
                        console.log("Selecting ward:", selectedWard);
                        $ward.val(selectedWard);
                    } else {
                        console.warn("Selected ward not found in list:", selectedWard);
                    }
                }
