    <script>
        (function ($) {
            const cache = {
                provinces: [],
                districts: {},
                wards: {}
            };

            const i18n = {
                selectProvince: "Chọn tỉnh/thành",
                selectDistrict: "Chọn quận/huyện",
                selectWard: "Chọn xã/phường",
                noDistricts: "Không có quận/huyện nào.",
                noWards: "Không có xã/phường nào.",
                apiError: "Lỗi khi gọi API."
            };

            const defaultProvinces = [
                { code: "01", name: "Hà Nội" },
                { code: "02", name: "Hồ Chí Minh" },
                { code: "56", name: "Bà Rịa - Vũng Tàu" },
                { code: "95", name: "Bạc Liêu" },
                { code: "04", name: "Hải Phòng" }
            ];

            function showError(msg) {
                alert(msg);
            }

            function toggleLoading($container, isLoading) {
                const $spinner = $container.find(".loading-spinner");
                if (isLoading) {
                    if (!$spinner.length) {
                        $container.prepend('<div class="loading-spinner" style="display: none;">Đang tải...</div>');
                    }
                    $spinner.show();
                } else {
                    $spinner.hide();
                }
            }

            function populateProvinces($province, provinces, selectedProvince) {
                if (!$province.length) {
                    console.error("Province dropdown not found!");
                    return;
                }
                console.log("Populating provinces:", provinces);
                $province.empty().append(`<option value="">${i18n.selectProvince}</option>`);
                provinces.forEach(p => {
                    $province.append(`<option value="${p.code}">${p.name}</option>`);
                });

                if (selectedProvince && selectedProvince !== "0") {
                    const provinceExists = provinces.some(p => p.code === selectedProvince);
                    if (provinceExists) {
                        console.log("Selecting province:", selectedProvince);
                        $province.val(selectedProvince);
                    } else {
                        console.warn("Selected province not found in list:", selectedProvince);
                    }
                }
            }

            function populateDistricts($district, districts, selectedDistrict) {
                if (!$district.length) {
                    console.error("District dropdown not found!");
                    return;
                }
                console.log("Populating districts:", districts);
                $district.empty().append(`<option value="">${i18n.selectDistrict}</option>`);
                districts.forEach(d => {
                    $district.append(`<option value="${d.code}">${d.name}</option>`);
                });

                if (selectedDistrict && selectedDistrict !== "0") {
                    const districtExists = districts.some(d => d.code === selectedDistrict);
                    if (districtExists) {
                        console.log("Selecting district:", selectedDistrict);
                        $district.val(selectedDistrict);
                    } else {
                        console.warn("Selected district not found in list:", selectedDistrict);
                    }
                }
            }

            function populateWards($ward, wards, selectedWard) {
                if (!$ward.length) {
                    console.error("Ward dropdown not found!");
                    return;
                }
                console.log("Populating wards:", wards);
                $ward.empty().append(`<option value="">${i18n.selectWard}</option>`);
