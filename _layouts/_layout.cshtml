                                $provinceName.val(settings.savedProvinceName);
                            } else {
                                $provinceName.val("");
                            }

                            if (settings.savedProvince && settings.savedProvince !== "0") {
                                const provinceExists = defaultProvinces.some(p => p.code === settings.savedProvince);
                                if (provinceExists) {
                                    $province.trigger("change");
                                }
                            }
                        },
                        complete: function () {
                            toggleLoading($container, false);
                        }
                    });

                    $province.on("change", function () {
                        console.log("Province changed to:", $(this).val());
                        const provinceId = $(this).val();
                        const provinceName = $province.find("option:selected").text();
                        $provinceName.val(provinceId ? provinceName : "");

                        $district.empty().append(`<option value="">${i18n.selectDistrict}</option>`);
                        $ward.empty().append(`<option value="">${i18n.selectWard}</option>`);
                        $districtName.val("");
                        $wardName.val("");

                        if (!provinceId) return;

                        if (cache.districts[provinceId]) {
                            populateDistricts($district, cache.districts[provinceId], settings.savedDistrict);
                            if (settings.savedDistrict && settings.savedDistrictName && settings.savedDistrict !== "0") {
                                $districtName.val(settings.savedDistrictName);
                            }
                            if (settings.savedDistrict && settings.savedDistrict !== "0") {
                                const districtExists = cache.districts[provinceId].some(d => d.code === settings.savedDistrict);
                                if (districtExists) {
                                    $district.trigger("change");
                                }
                            }
                            return;
                        }

                        toggleLoading($container, true);
                        $.ajax({
                            url: `https://vn-public-apis.fpo.vn/districts/getByProvince?provinceCode=${provinceId}`,
                            method: "GET",
                            timeout: 5000,
                            success: function (response) {
                                console.log("Districts API Response:", response);
                                let districts = response.data?.data || [];
                                if (!Array.isArray(districts) || !districts.length) {
                                    showError(i18n.noDistricts);
                                    return;
                                }
                                cache.districts[provinceId] = districts;
                                populateDistricts($district, districts, settings.savedDistrict);

                                if (settings.savedDistrict && settings.savedDistrictName && settings.savedDistrict !== "0") {
                                    $districtName.val(settings.savedDistrictName);
                                }
                                if (settings.savedDistrict && settings.savedDistrict !== "0") {
                                    const districtExists = districts.some(d => d.code === settings.savedDistrict);
                                    if (districtExists) {
                                        $district.trigger("change");
                                    }
                                }
                            },
                            error: function (xhr, status, error) {
                                console.error("Districts API Error:", status, error);
                                showError(i18n.apiError);
                            },
                            complete: function () {
                                toggleLoading($container, false);
                            }
                        });
                    });

                    $district.on("change", function () {
                        console.log("District changed to:", $(this).val());
                        const districtId = $(this).val();
                        const districtName = $district.find("option:selected").text();
                        $districtName.val(districtId ? districtName : "");

                        $ward.empty().append(`<option value="">${i18n.selectWard}</option>`);
                        $wardName.val("");

                        if (!districtId) return;

                        if (cache.wards[districtId]) {
                            populateWards($ward, cache.wards[districtId], settings.savedWard);
                            if (settings.savedWard && settings.savedWardName && settings.savedWard !== "0") {
                                $wardName.val(settings.savedWardName);
                            }
                            return;
                        }

                        toggleLoading($container, true);
                        $.ajax({
                            url: `https://vn-public-apis.fpo.vn/wards/getByDistrict?districtCode=${districtId}`,
                            method: "GET",
                            timeout: 5000,
                            success: function (response) {
                                console.log("Wards API Response:", response);
                                let wards = response.data?.data || [];
                                if (!Array.isArray(wards) || !wards.length) {
                                    showError(i18n.noWards);
                                    return;
                                }
                                cache.wards[districtId] = wards;
                                populateWards($ward, wards, settings.savedWard);

                                if (settings.savedWard && settings.savedWardName && settings.savedWard !== "0") {
                                    $wardName.val(settings.savedWardName);
                                }
                            },
                            error: function (xhr, status, error) {
                                console.error("Wards API Error:", status, error);
                                showError(i18n.apiError);
                            },
                            complete: function () {
                                toggleLoading($container, false);
                            }
                        });
                    });

                    $ward.on("change", function () {
                        console.log("Ward changed to:", $(this).val());
                        const wardId = $(this).val();
                        const wardName = $ward.find("option:selected").text();
                        $wardName.val(wardId ? wardName : "");
                    });
                });
            };
        })(jQuery);
    </script>
    @RenderSection("scripts", required: false)

    <script>
        let lastScrollTop = 0;
        const header = document.querySelector('.top_panel');

        window.addEventListener('scroll', function () {
            let scrollTop = window.pageYOffset || document.documentElement.scrollTop;

            if (scrollTop > lastScrollTop) {
                // Đang cuộn xuống
                header.classList.remove('show-on-scroll');
                header.classList.add('hide-on-scroll');
            } else {
                // Đang cuộn lên
                header.classList.remove('hide-on-scroll');
                header.classList.add('show-on-scroll');
            }

            lastScrollTop = scrollTop <= 0 ? 0 : scrollTop;
        });
    </script>
  

</body>
</html>